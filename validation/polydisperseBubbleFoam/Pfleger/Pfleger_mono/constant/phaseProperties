FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (air water);

air
{
    type            polydispersePhaseModel;
    residualAlpha   1e-4;
    minD            1e-4;
    maxD            1e-2;
}

water
{
    type            monodispersePhaseModel;
    residualAlpha   1e-4;
    d               1e-4;
}

pMin    1e3;
pMax    1e7;

blending
{
    default
    {
        type                            none;
        continuousPhase                 water;
        dispersedPhase                  air;
    }
}

sigma
(
    (air and water)   0.07
);

aspectRatio
(
);

drag
(
    (air in water)
    {
        type            SchillerNaumann;
        residualRe      1e-3;
        residualEo      1e-3;
        residualE       1e-2;
        swarmCorrection
        {
            type        none;
        }
    }
);


turbulentDispersion
(
);

virtualMass
(
);

lift
(
);

wallLubrication
(
);

bubblePressure
(
);
