FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha.air2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.34;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.34;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.air2;
        inletValue      uniform 0.34;
    }
    walls
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type    empty;
    }
}

